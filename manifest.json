{
    "name":"ClipboardX",
    "description":"A chrome extension, work around for a clipboard",
    "manifest_version":2,
    "version":"1.0",
    "content_scripts": [ {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      } ],
    
    "browser_action":{
        "default_icon":{
            "16":"icon.png",
            "24":"icon.png",
            "32":"icon.png"
        },
        "default_title":"ClipboardX",
        "default_popup":"popup.html"
    },
    
  "permissions":["storage","tabs"]
}
